<%= form_for @job_offer do |f| %>
  <%= f.label :title, "Título" %>
  <%= f.text_field :title %>
  <%= f.label :description, "Descripción" %>
  <%= f.text_area :description %>
  <%= f.label :requirements, "Requisitos" %>
  <%= f.text_area :requirements %>
  <%= f.submit "Crear Oferta Laboral" %>
<% end %>
